{
	"name": "aero",
	"version": "0.1.6",
	"description": "CMS based on Node, Express, Jade and Stylus.",
	"main": "aero.js",
	"dependencies": {
		"express": "*",
		"jade": "*",
		"stylus": "*",
		"nib": "*",
		"object-assign": "*",
		"compression": "*",
		"uglify-js": "*",
		"node-watch": "*"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blitzprog/aero.git"
	},
	"keywords": [
		"website",
		"cms",
		"express",
		"jade",
		"stylus"
	],
	"eslintConfig": {
		"env": {
			"node": true
		},
		"rules": {
			"eqeqeq": 2,
			"curly": 0,
			"quotes": [2, "double"],
			"no-trailing-spaces": 0,
			"eol-last": 0
		}
	},
	"author": "Eduard Urbach <e.urbach@gmail.com> (http://blitzprog.org)",
	"license": "GPLv2",
	"bugs": {
		"url": "https://github.com/blitzprog/aero/issues"
	},
	"homepage": "https://github.com/blitzprog/aero"
}